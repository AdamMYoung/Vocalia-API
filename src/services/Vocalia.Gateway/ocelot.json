{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "vocalia.podcast",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/podcast/{everything}",
      "UpstreamHttpMethod": [ "GET", "DELETE", "POST", "PUT" ]
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "vocalia.social",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/social/{everything}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/voice/{catchAll}",
      "DownstreamScheme": "ws",
      "DownstreamHostAndPorts": [
        {
          "Host": "vocalia.ingest",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/ingest/voice/{catchAll}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://api.vocalia.co.uk"
  }
}
